#!/usr/bin/env node
/**
 * Generate node module API markdown with dox
 *
 * Licensed under MIT.
 * Copyright (c) 2013 David Smith <https://github.com/codeactual/>
 */

var commander = require('commander');

commander
  .option('-r, --rootdir <dir>', 'Project/repo root directory', String, process.cwd())
  .option('-f, --file <name>', 'Source file', String);

require('impulse-bin').create().run(commander, require('../lib/cli/gitemplate-dox'));
